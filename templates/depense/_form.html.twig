{{ form_start(form) }}
<div class="row">
    <div class="col-md-6">{{ form_row(form.typedepense) }}</div>
    <div class="col-md-6">{{ form_row(form.title) }}</div>
</div>
<div class="row">
    <div class="col-md-4">{{ form_row(form.amount) }}</div>
    <div class="col-md-4">{{ form_row(form.employee) }}</div>
    <div class="col-md-4">{{ form_row(form.date_depense) }}</div>
</div>
{{ form_rest(form) }}
<button class="btn btn-primary">{{ button | default('Enregistrer') }}</button>
{{ form_end(form) }}